{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
    },
    "overrides": [
        {
            "files": ["tests/*.spec.js", "tests/*.test.js"], // Or *.test.js
            "rules": {
                "require-jsdoc": "off"
            }
        }
    ]
}
